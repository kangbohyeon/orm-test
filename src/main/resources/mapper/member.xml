<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.performance.sample.repository.MybatisRepositoryImpl">
    <insert id="save" parameterType="com.performance.sample.domain.Member">
        INSERT INTO t_member (name, age, address, gender)
        VALUES (#{name},#{age},#{address},#{gender})
    </insert>
</mapper>